# Server Configuration
PORT=3402
NODE_ENV=production

# Smart Router Provider API Keys (optional)
# At least 1 key required to enable Smart Router
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
DEEPSEEK_API_KEY=

